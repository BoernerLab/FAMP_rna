 author = {{\v{S}}poner, Ji{\v{r}}{\'i} and Bussi, Giovanni and Krepl, Miroslav and Ban{\'a}{\v{s}}, Pavel and Bottaro, Sandro and Cunha, Richard A. and Gil-Ley, Alejandro and Pinamonti, Giovanni and Poblete, Sim{\'o}n and Jure{\v{c}}ka, Petr and Walter, Nils G. and Otyepka, Michal},
